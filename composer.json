{
    "name": "hrupin/yii2-blog",
    "description": "Yii2 Blog for other application YII2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hrupin Aleksandr",
            "email": "hrupin.work@yandex.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "^2.0.0",
        "yiisoft/yii2-swiftmailer": "^2.0.0",
        "yiisoft/yii2-bootstrap": "^2.0.0",
        "mihaildev/yii2-ckeditor": "*"
    },    
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0.0",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "hrupin\\blog\\": ""
        }
    },
    "extra": {
        "bootstrap": "hrupin\\blog\\Bootstrap"
    }
}
